{% extends 'base.html.twig' %}

{% block title %}Prestamos{% endblock %}

{% block body %}
    <h1>Prestamos</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prestamo.id }}</td>
            </tr>
            <tr>
                <th>Fecha_Prestamo</th>
                <td>{{ prestamo.FechaPrestamo ? prestamo.FechaPrestamo|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Fecha_Devolucion</th>
                <td>{{ prestamo.FechaDevolucion ? prestamo.FechaDevolucion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Estado_Prestamo</th>
                <td>{{ prestamo.EstadoPrestamo }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_prestamos_index') }}">back to list</a>

    <a href="{{ path('app_prestamos_edit', {'id': prestamo.id}) }}">edit</a>

    {{ include('prestamos/_delete_form.html.twig') }}
{% endblock %}
